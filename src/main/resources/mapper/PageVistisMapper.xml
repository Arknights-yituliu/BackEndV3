<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lhs.mapper.PageVisitsMapper">

    <!--批量插入访问量-->

    <insert id="insertBatch">
        INSERT INTO `page_visits`
        (`id`, `page_path`, `visits_time`, `visits_count`, `create_time`, `redis_key`)
        VALUES
        <foreach collection="list" item="item" separator=",">
            (#{item.id},
            #{item.pagePath},
            #{item.visitsTime},
            #{item.visitsCount},
            #{item.createTime},
            #{item.redisKey})
        </foreach>
    </insert>

</mapper>